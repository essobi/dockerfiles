### RNEURO  
FROM muschellij2/rneuro_ants  
  
#####################  
# Getting Kirby data stuff  
#####################  
RUN r -e 'devtools::install_github("muschellij2/kirby21.base")'  
RUN r -e 'devtools::install_github("muschellij2/kirby21.t1")'  
RUN r -e 'devtools::install_github("muschellij2/kirby21.asl")'  
RUN r -e 'devtools::install_github("muschellij2/kirby21.det2")'  
RUN r -e 'devtools::install_github("muschellij2/kirby21.dti")'  
RUN r -e 'devtools::install_github("muschellij2/kirby21.flair")'  
RUN r -e 'devtools::install_github("muschellij2/kirby21.fmri")'  
RUN r -e 'devtools::install_github("muschellij2/kirby21.mt")'  
RUN r -e 'devtools::install_github("muschellij2/kirby21.survey")'  
RUN r -e 'devtools::install_github("muschellij2/kirby21.t2")'  
RUN r -e 'devtools::install_github("muschellij2/kirby21.vaso")'  
RUN r -e 'devtools::install_github("muschellij2/kirby21.smri")'  

