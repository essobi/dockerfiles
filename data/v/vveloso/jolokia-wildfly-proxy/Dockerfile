war/1.3.3/jolokia-war-1.3.3.war /usr/local/tomcat/webapps/jolokia.war  
ADD http://central.maven.org/maven2/org/wildfly/wildfly-client-
all/10.0.0.Final/wildfly-client-all-10.0.0.Final.jar
/usr/local/tomcat/lib/wildfly-client-all.jar  

