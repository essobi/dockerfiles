FROM alpine  
  
RUN apk add \--update ca-certificates python python-dev py-pip wget bash
linux-headers musl-dev libffi-dev openssl-dev gcc make libxml2-utils curl && \  
pip install paramiko && \  
mkdir -p /scripts && \  
cd /scripts && \  
wget https://raw.githubusercontent.com/tail-f-
systems/JNC/master/examples/2-junos/netconf-console  
  
CMD ["echo"]  

