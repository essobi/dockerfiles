./configure && make && make install && cd \- && rm -fr cphalcon* && docker-
php-ext-enable phalcon; \  
fi; \  
if [ 5 -eq 7 ]; then \  
wget -O cphalcon.zip https://github.com/phalcon/cphalcon/archive/master.zip &&
