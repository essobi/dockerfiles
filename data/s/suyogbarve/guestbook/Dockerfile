ADD index.php /var/www/index.php  
ADD angular.min.js /var/www/angular.min.js  
ADD bootstrap.min.css /var/www/bootstrap.min.css  
ADD controllers.js /var/www/controllers.js  
ADD ui-bootstrap-tpls-0.13.0.min.js /var/www/ui-bootstrap-tpls-0.13.0.min.js  
ADD index.html /var/www/index.html  
  
CMD /run.sh  

