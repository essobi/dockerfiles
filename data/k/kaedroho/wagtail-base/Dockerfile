FROM ubuntu:14.04  
MAINTAINER Karl Hobley <karlhobley10@gmail.com>  
  
RUN apt-get update -y  
RUN apt-get install -y build-essential python python-dev python-setuptools
libpq-dev libjpeg-dev libtiff-dev zlib1g-dev libfreetype6-dev liblcms2-dev
python-opencv python-numpy  
RUN easy_install -U pip  

