COPY playbooks /opt/apb/actions  
COPY roles /opt/ansible/roles  
RUN chmod -R g=u /opt/{ansible,apb} && \  
find /opt/ansible/roles/keycloak-apb -type f | xargs sed -i
"s/'persistent'/'prod'/g" && \  
find /opt/ansible/roles/keycloak-apb -type f | xargs sed -i
"s/'ephemeral'/'dev'/g"  
  
