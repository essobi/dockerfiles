# This Dockerfile is generated by "update.sh"  
# Do not edit it locally - your changes will be lost!  
  
FROM openjdk:7-jdk  
  
ENV SCALA_VERSION 2.10.6  
RUN curl "https://www.scala-lang.org/files/archive/scala-$SCALA_VERSION.deb"
-o scala.deb && \  
( dpkg -i scala.deb || true ) && \  
apt-get update -y && apt-get install -y -f --no-install-recommends && \  
rm scala.deb && \  
rm -rf /var/lib/apt/lists/*  

